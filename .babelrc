{
  "presets": [
    ["env", {
      "targets": {
        "node": "6.0.0"
      }
    }]
  ],
  "plugins": [
    "transform-decorators-legacy",
    "transform-class-properties",
    "transform-object-rest-spread",
    ["module-resolver", {
      "cwd": "babelrc",
      "root": ["./src"]
    }]
  ],
  "comments": false,
  "env": {
    "development": {
      "ignore": "spec"
    },
    "test": {
      "sourceMaps": "inline"
    }
  }
}
